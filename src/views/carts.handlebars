<div>
    <h1>Cart</h1>
    <b>Cart ID: {{cart._id}}</b>
    <ul>
        {{#each cart.products}}
            <b><h3>{{product.title}}</h3></b>
            <li>{{product.description}}</li>
            <li>{{product.price}}</li>
            <li>{{product.code}}</li>
            <li>{{product.stock}}</li>
            <li>{{product.status}}</li>
            <li>{{product.category}}</li>
            <br>
        {{/each}}
    </ul>
</div>
